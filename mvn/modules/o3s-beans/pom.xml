        <scope>provided</scope>
    </dependency>
    <dependency>
        <groupId>org.ow2.spec.ee</groupId>
        <artifactId>ow2-ejb-3.0-spec</artifactId>
        <version>${ow2.spec.version}</version>
        <scope>provided</scope>
    </dependency>
    <dependency>
    	<groupId>net.o3s</groupId>
    	<artifactId>o3s-persistence</artifactId>

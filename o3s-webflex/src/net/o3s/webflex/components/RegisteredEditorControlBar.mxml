<mx:Canvas xmlns:mx="http://www.adobe.com/2006/mxml"
		   xmlns:components="net.o3s.webflex.components.*"
		   creationComplete="init()"
		   width="100%">
<mx:Script>
	<![CDATA[

		private function init():void {

		}
	]]>
</mx:Script>

	<mx:HBox id="HB_Submit" width="100%" height="10%">
		<mx:Panel title="Inscription" width="100%" height="100%">
			<mx:ControlBar horizontalAlign="center">
				<mx:Button
					id="submitRegisteredButton"
					label="Soumettre inscription"
					click="this.parentDocument.submitRegisteredForm(event);"
					enabled="{this.parentDocument.submitRegisteredButtonEnabled} "
					/>
				<mx:Button
					label="Annuler"
					click="this.parentDocument.clearAll();"
					/>
			</mx:ControlBar>
		</mx:Panel>
	</mx:HBox>
</mx:Canvas>

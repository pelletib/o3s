			<mx:Panel width="100%" height="100%" horizontalAlign="center" title="Liste des inscrits">
				<mx:DataGrid id="DG_registereds" dataProvider="{RO_registering.getRegistereds.lastResult}" dataTipFunction="buildRegisteredTip"
